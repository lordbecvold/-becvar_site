{% extends 'common/bases/admin-base.html.twig' %}

{% block title %}
    confirmation
{% endblock %}

{% block component %}
<div class="admin-panel">
    <div class="confirmation dark-table">
        <p class="form-title">Are you sure you want to delete logs?</p>
        <a class="confirmation-button" href="/admin/database/delete?table=logs&page={{ page }}&id=all&referer=log_reader">Yes</a>
        <a class="confirmation-button" href="/admin/logs?page={{ page }}">No</a>
    </div>
</div>
{% endblock %}
